{
     "version": 2,
     "builds": [
       {
         "src": "src/my_test_app/main.py",
         "use": "@vercel/python"
       },
       {
         "src": "frontend/build/**/*",
         "use": "@vercel/static"
       }
     ],
     "routes": [
       {
         "src": "/api/(.*)",
         "dest": "src/my_test_app/main.py"
       },
       {
         "src": "/(.*)",
         "dest": "frontend/build/$1"
       }
     ],
     "env": {
       "APP_MODULE": "src.my_test_app.main:app"
     }
   }